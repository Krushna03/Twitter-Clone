
generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
  firstName String 
  lastname String?
  email String @unique
  profileImage String?
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt  
}

